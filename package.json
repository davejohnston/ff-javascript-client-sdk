{
  "name": "@harnessio/ff-javascript-client-sdk",
  "version": "1.0.2",
  "author": "Harness",
  "license": "Apache-2.0",
  "main": "dist/sdk.cjs.js",
  "module": "dist/sdk.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build:esm": "esbuild ./src/index.ts --bundle  --sourcemap --format=esm --outfile=./dist/sdk.esm.js",
    "build:node": "esbuild ./src/index.ts --bundle  --sourcemap --format=cjs --outfile=./dist/sdk.cjs.js",
    "build": "npm run clean; npm run type; npm run build:esm; npm run build:node",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "type": "tsc src/*.ts --declaration --emitDeclarationOnly --outDir dist --lib ES2015",
    "clean": "rm -rf ./dist",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "esbuild": "^0.9.0",
    "prettier": "^2.2.1",
    "tslib": "^2.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2",
    "@types/es6-promise": "^3.3.0",
    "@types/eventsource": "^1.1.5",
    "cross-fetch": "^3.0.6",
    "event-source-polyfill": "^1.0.22",
    "jwt-decode": "^3.1.2",
    "mitt": "^2.1.0"
  },
  "description": "Basic library for integrating CF into javascript applications.",
  "dependencies": {
    "ansi-styles": "^3.2.1",
    "balanced-match": "^1.0.0",
    "builtin-modules": "^1.1.1",
    "brace-expansion": "^1.1.11",
    "chalk": "^2.4.2",
    "commander": "^2.20.3",
    "color-convert": "^1.9.3",
    "diff": "^4.0.2",
    "escape-string-regexp": "^1.0.5",
    "color-name": "^1.1.3",
    "es6-promise": "^4.2.8",
    "function-bind": "^1.1.1",
    "glob": "^7.1.6",
    "fs.realpath": "^1.0.0",
    "has": "^1.0.3",
    "inflight": "^1.0.6",
    "inherits": "^2.0.4",
    "is-core-module": "^2.2.0",
    "has-flag": "^3.0.0",
    "js-yaml": "^3.14.1",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "esprima": "^4.0.1",
    "argparse": "^1.0.10",
    "js-tokens": "^4.0.0",
    "mkdirp": "^0.5.5",
    "concat-map": "^0.0.1",
    "path-is-absolute": "^1.0.1",
    "once": "^1.4.0",
    "node-fetch": "^2.6.1",
    "path-parse": "^1.0.6",
    "supports-color": "^5.5.0",
    "sprintf-js": "^1.0.3",
    "resolve": "^1.19.0",
    "tsutils": "^2.29.0",
    "semver": "^5.7.1",
    "wrappy": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drone/ff-javascript-client-sdk.git"
  },
  "keywords": [
    "Harness",
    "Feature",
    "Flags",
    "Continuous",
    "Features",
    "CD",
    "CI",
    "CF",
    "Drone"
  ],
  "bugs": {
    "url": "https://github.com/drone/ff-javascript-client-sdk/issues"
  },
  "homepage": "https://github.com/drone/ff-javascript-client-sdk#readme"
}
